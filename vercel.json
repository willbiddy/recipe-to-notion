{
	"$schema": "https://openapi.vercel.sh/vercel.json",
	"bunVersion": "1.x",
	"buildCommand": "bun run build:web",
	"outputDirectory": ".",
	"cleanUrls": true,
	"trailingSlash": false,
	"rewrites": [
		{
			"source": "/",
			"destination": "/api/index"
		},
		{
			"source": "/(web\\.js|web\\.css|manifest\\.json|fork-and-knife\\.png|favicon.*\\.png|apple-touch-icon\\.png)",
			"destination": "/public/$1"
		}
	],
	"functions": {
		"api/**/*.ts": {
			"maxDuration": 60
		}
	}
}
