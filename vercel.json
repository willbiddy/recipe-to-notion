{
	"$schema": "https://openapi.vercel.sh/vercel.json",
	"bunVersion": "1.x",
	"buildCommand": "",
	"outputDirectory": ".",
	"rewrites": [
		{
			"source": "/",
			"destination": "/public/index.html"
		},
		{
			"source": "/(.*\\.(js|css|json|png|jpg|svg))",
			"destination": "/public/$1"
		}
	],
	"functions": {
		"api/**/*.ts": {
			"maxDuration": 60
		}
	}
}
